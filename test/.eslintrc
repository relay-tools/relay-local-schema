{
  "extends": [
    "4catalyzer-react",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "prettier/prettier": "error",
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": true
    }],
    "react/no-multi-comp": "off",
    "react/prop-types": "off"
  }
}
